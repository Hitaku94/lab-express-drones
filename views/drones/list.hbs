<h2>Available drones</h2>

    {{#each data}}
<div class="list">
<ul>
    <li>Name: {{this.name}}</li>
    <li>Propellers: {{this.propellers}}</li>
    <li>Speed: {{this.maxSpeed}}</li>
    <br>
    <form action="/drones/{{this.id}}/delete" method="POST">
        <a class="navbar-brand" href="/drones/{{this.id}}/edit">Edit</a>
        <button type="submit" class="btn btn-warning">Delete</button>
    </form>
    <br>
    <br>
</ul>
</div>
    {{/each}}
